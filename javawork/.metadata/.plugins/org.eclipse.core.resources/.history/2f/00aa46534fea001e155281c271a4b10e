package day0325;

public class Student {
	private String name;
	private int java;
	private int spring;
	
	// 4개의 생성자
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Student(String name) {
		super();
		this.name = name;
	}

	public Student(int java, int spring) {
		super();
		this.java = java;
		this.spring = spring;
	}

	public Student(String name, int java, int spring) {
		super();
		this.name = name;
		this.java = java;
		this.spring = spring;
	}
	
	
}
